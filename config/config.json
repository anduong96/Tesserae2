{
    "requestURI": "/",

    "express":{
        "x-powered-by":false,
        "trust proxy":true,
        "jsonp callback name":null,
        "json replacer":null,
        "json spaces":0,
        "case sensitive routing":false,
        "strict routing":false,
        "view cache":false,
        "view engine":"js",
        "views":"path:./client",
        "view":"require:react-engine/lib/expressView",
        "mountpath": "config:requestURI"
    },

    "view engines": {
        "js":{
            "module":"react-engine/lib/server",
            "renderer":{
                "method":"create",
                "arguments":[
                    {
                        "docType": "<!DOCTYPE html>",
                        "routes":"require:./client/RouterConfig",
                        "routesFilePath":"path:./client/RouterConfig.js"
                    }
                ]
            }
        }
    },

    "middleware": {
        "logger":{
            "enabled":true,
            "priority":30,
            "module":{
                "name":"morgan",
                "arguments":[
                    "tiny"
                ]
            }
        },

        "static":{
            "enabled":true,
            "priority":40,
            "module":{
                "arguments":[
                    "path:./dist"
                ]
            }
        },

        "urlencoded":{
            "enabled":true,
            "priority":70,
            "module": {
                "name": "body-parser",
                "method": "json",
                "arguments":[
                    {
                        "parameterLimit": 10000,
                        "limit": 10485760,
                        "extended": true
                    }
                ]
            }
        },

        "json": {
            "enabled": true,
            "priority": 60,
            "module": {
                "name": "body-parser",
                "method": "json",
                "arguments":[
                    {
                        "parameterLimit": 10000,
                        "limit": 10485760,
                        "extended": true
                    }
                ]
            }
        },

        "router":{
            "enabled":true,
            "priority":120,
            "module":{
                "arguments":[
                    {
                        "directory":"path:./routes"
                    }
                ]
            }
        }
    }
}
